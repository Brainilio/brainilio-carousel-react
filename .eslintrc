{
     "plugins": [
          "react"
     ],
     "parser": "babel-eslint",
     "parserOptions": {
          "ecmaVersion": 6,
          "sourceType": "module",
          "ecmaFeatures": {
               "jsx": true
          }
     },
     "env": {
          "es6": true,
          "browser": true,
          "node": true,
          "mocha": true
     },
     "extends": [
          "eslint:recommended",
          "plugin:react/recommended"
        
     ],
     "rules": {
          "no-console": 1,
          "react/jsx-uses-react": "error",
          "react/jsx-uses-vars": "error"
     },
     "settings": {
          "react": {
               "version": "16.3"
          }
     }
}